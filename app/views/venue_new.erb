</nav>
  </div><br>
    <div class="container">
        <div class="row">
            <form method="post" action="/venues" class="form-horizontal" role="form" >
              <div class="col-md-6">
                <div >
                    <label for="title" class="col-sm-2 control-label">Date</label>
                    <input type="date" name="date">
                </div>
                <br>
                <div class="form-group">
                    <label for="title" class="col-sm-2 control-label">Title</label>
                    <div class="col-sm-8">
                      <input type="title" class="form-control" name="title" id="title" placeholder="Title">
                    </div>
                </div>
                <div class="form-group">
                    <label for="address" class="col-sm-2 control-label">Address</label>
                    <div class="col-sm-8">
                      <input type="address" class="form-control" name="address" id="address" placeholder="Address">
                    </div>
                </div>
                <div class="form-group">
                    <label for="city" class="col-sm-2 control-label">City</label>
                    <div class="col-sm-8">
                      <input type="city" class="form-control" name="city" id="city" placeholder="City">
                    </div>
                </div>
                <div class="form-group">
                    <label for="zip" class="col-sm-2 control-label">Zip Code</label>
                    <div class="col-sm-8">
                      <input type="zip" class="form-control" name="zip" id="zip" placeholder="Zip Code">
                    </div>
                </div>
                 <div class="form-group">
                    <label for="state" class="col-sm-2 control-label">state </label>
                    <div class="col-sm-8">
                      <input type="state" class="form-control" name="state" id="state" placeholder="state">
                    </div>
                </div>
              </div>
              <div class="col-md-6">
                <div class="form-group col-sm-8">
                  <select class="form-control" name="area">
                    <option value="contro_costo">Contro Costa</option>
                    <option value="humboldt">Humboldt</option>
                    <option value="marin">Marin</option>
                    <option value="Mendocino">Mendocino</option>
                    <option value="monterey">Monterey</option>
                    <option value="south_bay">South Bay</option>
                    <option value="san_francisco">San Francisco</option>
                    <option value="east_bay">East Bay</option>
                  </select>
                </div>
                 <div class="form-group col-sm-8">
                  <select class="form-control" name="event_type">
                    <option value="milongas"> Milongas</option>
                    <option value="drop_in_classes">Drop-in Classes</option>
                    <option value="series_worskops">Series/Worskops</option>
                    <option value="practicas">Practicas</option>
                    <option value="live_music">Live Music</option>
                    <option value="performances">Performances</option>
                  </select>
                </div>
                <div class="form-group col-sm-8">
                  <textarea class="form-control" rows="5"></textarea>
                </div>
             </div>
          </div>
                <div class="form-group col-md-offset-5">
               <div class="col-sm-8 control-label"><input class="btn btn-default " type="submit" value="Submit">
               </div>
          </form>
      </div>
    </div>
